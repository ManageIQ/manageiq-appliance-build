# build time repos - these repos are used to install the initial packages
repo --name=baseos     --baseurl=https://mirror.stream.centos.org/9-stream/BaseOS/x86_64/os/
repo --name=appstream  --baseurl=https://mirror.stream.centos.org/9-stream/AppStream/x86_64/os/
repo --name=crb        --baseurl=https://mirror.stream.centos.org/9-stream/CRB/x86_64/os/
repo --name=epel       --mirrorlist=https://mirrors.fedoraproject.org/mirrorlist?repo=epel-9&arch=x86_64

repo --name=manageiq-19-spassky         --baseurl=https://rpm.manageiq.org/release/19-spassky/el$releasever/$basearch
repo --name=manageiq-19-spassky-noarch  --baseurl=https://rpm.manageiq.org/release/19-spassky/el$releasever/noarch
<% if @build_type != "release" %>
repo --name=manageiq-19-spassky-nightly --baseurl=https://rpm.manageiq.org/release/19-spassky-nightly/el$releasever/$basearch
<% end %>

<% if @target == "gce" %>
repo --name=google-compute-engine --baseurl=https://packages.cloud.google.com/yum/repos/google-compute-engine-el9-x86_64-stable
<% end %>

<%= render_partial_if_exist "main/repos-extra" %>
